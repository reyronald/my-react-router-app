{
  "name": "my-react-router-app",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "clean": "rm -rf build",
    "dev": "react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "prettier:check": "prettier . -c",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate",
    "db:migrate:create": "prisma migrate dev -- --create-only",
    "db:reset": "prisma migrate reset",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "lint": "eslint .",
    "lint:ci": "eslint . --max-warnings=0",
    "lint:file": "eslint",
    "lint:changed": "git diff --name-only --diff-filter=d --cached | grep -E \"(.*)\\.(jsx|js|ts|tsx)$\" | xargs npm run lint:file -- --color",
    "lint:branch": "git diff --name-only --diff-filter=d --cached main | grep -E \"(.*)\\.(jsx|js|ts|tsx)$\" | xargs npm run lint:file -- --color",
    "typecheck": "react-router typegen && tsc",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "danger": "danger"
  },
  "dependencies": {
    "@epic-web/remember": "^1.1.0",
    "@prisma/client": "^6.0.1",
    "@react-router/node": "^7.0.0",
    "@react-router/serve": "^7.0.0",
    "@tanstack/react-query": "^5.61.0",
    "chalk": "^5.3.0",
    "deepmerge": "^4.3.1",
    "isbot": "^5.1.17",
    "prisma": "^6.0.1",
    "prisma-query-log": "^3.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^7.0.1",
    "superjson": "^2.2.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.2",
    "@eslint-react/eslint-plugin": "^1.17.1",
    "@eslint/js": "^9.16.0",
    "@react-router/dev": "^7.0.1",
    "@storybook/addon-essentials": "^8.4.5",
    "@storybook/addon-interactions": "^8.4.5",
    "@storybook/addon-onboarding": "^8.4.5",
    "@storybook/blocks": "^8.4.5",
    "@storybook/react": "^8.4.5",
    "@storybook/react-vite": "^8.4.5",
    "@storybook/test": "^8.4.5",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "@types/react": "^18.3.9",
    "@types/react-dom": "^18.3.0",
    "@vitest/coverage-v8": "^2.1.6",
    "@vitest/eslint-plugin": "^1.1.12",
    "@vitest/ui": "^2.1.6",
    "autoprefixer": "^10.4.20",
    "close-with-grace": "^2.1.0",
    "danger": "^12.3.3",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import-x": "^4.4.3",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-compiler": "^19.0.0-beta-df7b47d-20241124",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-testing-library": "^7.0.0",
    "globals": "^15.12.0",
    "happy-dom": "^15.11.7",
    "msw": "^2.6.6",
    "msw-storybook-addon": "^2.0.4",
    "postcss": "^8.4.47",
    "storybook": "^8.4.5",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.16.0",
    "vite": "^5.4.8",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.1.6"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.6.1"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "prisma": {
    "schema": "prisma/schema.prisma",
    "seed": "node --experimental-strip-types prisma/seed.ts"
  }
}
